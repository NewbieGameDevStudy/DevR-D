        <Canvas Name="GameCanvans" HorizontalAlignment="Left" Height="399" Margin="10,10,0,0" VerticalAlignment="Top" Width="375" Background="Aqua" MouseLeftButtonDown="GameCanvans_MouseLeftButtonDown"/>
